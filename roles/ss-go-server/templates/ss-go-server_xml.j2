<?xml version="1.0" encoding="utf-8"?>
<service>
  <short>ss-go-server</short>
  <description> ss-go-server port </description>
{% set end_port = 10000 + port_nums %}
  <port protocol="tcp" port="10000-{{ end_port }}"/>
  <port protocol="udp" port="10000-{{ end_port }}"/>
{% set end_port = 20000 + port_nums %}
  <port protocol="tcp" port="20000-{{ end_port }}"/>
  <port protocol="udp" port="20000-{{ end_port }}"/>
</service>
